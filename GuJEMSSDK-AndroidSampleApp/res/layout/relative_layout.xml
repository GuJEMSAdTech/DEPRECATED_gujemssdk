<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ems="http://schemas.android.com/apk/res/de.guj.ems.mobile.sdk.test"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon" />

    <EditText
        android:id="@+id/sampleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="none"
        android:text="Dies ist ein Textfeld, ein Textfeld, ein Textfeld" >
    </EditText>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/sampleText" >
 
        <de.guj.ems.mobile.sdk.views.GuJEMSAdView
            android:id="@+id/ad15312"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            ems:ems_geo="false"
            ems:ems_kw="false"
            ems:ems_nkw="false"
            ems:ems_uid="true"
            ems:ems_zoneId="17944"
            ems:ems_bfSiteId="44496"
            ems:ems_bfZoneId="174882" />
   
<!--  ems:ems_zoneId="15312" -->
        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
             android:layout_below="@id/ad15312"
            android:src="@drawable/de5a9f0900" />
    </RelativeLayout>

</RelativeLayout>